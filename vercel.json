{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    },
    {
      "src": "ui/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/ui/$1"
    }
  ]
}
